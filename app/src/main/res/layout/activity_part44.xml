        android:text="Treat any wood you use before using it, be aware and informed what you use for treatment not every coating and treatment is suitable to be in contact with food products."
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="309dp" />

    <TextView
        android:id="@+id/textView37"
        organic
        android:layout_width="259dp"
        android:layout_height="99dp"
        android:text="*Untreated wood can degrade in as little as 3 years.
                      *Choose your wood treatment carefully.Some wood treatments require that the food is no longer listed as "organic" or safe for consumption because of the use of chemicals.
                      *Consider a treatment like Erdalith, which has limited leaching properties.
                      *Use metal supports in place of wood supports whenever possible."
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="463dp""
        app:layout_constraintBottom_toTopOf="@+id/button16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout16"
        app:layout_constraintVertical_bias="0.467" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="52dp"
        android:text="NEXT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
